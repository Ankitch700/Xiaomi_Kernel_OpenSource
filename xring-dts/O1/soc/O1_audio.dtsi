&dw_i2c0 {
	cs35l41b: speaker@40 {
		status = "okay";
		compatible = "cs35l41b";
		reset-gpios = <&dw_gpio4 3 1>;
		reg = <0x40>;
		interrupt-parent = <&gic>;
		interrupts = <GIC_SPI 151 IRQ_TYPE_LEVEL_HIGH>;
	};
	/* BOT Smart PA */
	spk1: cs35l43@40 {
		status = "disabled";
		compatible = "cirrus,cs35l43";
		#sound-dai-cells = <0>;
		reg = <0x40>;
		reset-gpios = <&dw_gpio4 3 0>;
		irq-gpio = <&dw_gpio3 31 0x0>;
		#interrupt-parent = <&dw_gpio3>;
		#interrupts = <31 IRQ_TYPE_LEVEL_LOW>;
		VA-supply = <&mldo8>;
		sound-channel = <0>;
		sound-name-prefix = "B";
		cirrus,i2c-addr = <40>;
		cirrus,bst-ipk-ma = <4000>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,low-pwr-mode-standby = <1>;
		cirrus,vpbr-enable = <1>;
		cirrus,vpbr-rel-rate = <0>;
		cirrus,vpbr-wait = <0>;
		cirrus,vpbr-atk-rate = <0>;
		cirrus,vpbr-atk-vol = <0x5>;
		cirrus,vpbr-max-att = <0x3>;
		cirrus,vpbr-thld = <0x8>;
		cirrus,gpio2-src-sel = <2>;
	};
	/* TOP Smart PA */
	spk2: cs35l43@42 {
		status = "disabled";
		compatible = "cirrus,cs35l43";
		#sound-dai-cells = <0>;
		reg = <0x42>;
		reset-gpios = <&dw_gpio4 3 0>;
		irq-gpio = <&dw_gpio4 0 0x0>;
		#interrupt-parent = <&dw_gpio4>;
		#interrupts = <0 IRQ_TYPE_LEVEL_LOW>;
		VA-supply = <&mldo8>;
		sound-channel = <1>;
		sound-name-prefix = "T";
		cirrus,i2c-addr = <42>;
		cirrus,bst-ipk-ma = <4000>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,low-pwr-mode-standby = <1>;
		cirrus,vpbr-enable = <1>;
		cirrus,vpbr-rel-rate = <0>;
		cirrus,vpbr-wait = <0>;
		cirrus,vpbr-atk-rate = <0>;
		cirrus,vpbr-atk-vol = <0x5>;
		cirrus,vpbr-max-att = <0x3>;
		cirrus,vpbr-thld = <0x8>;
		cirrus,gpio2-src-sel = <2>;
    };
	spk3: cs35l43@41 {
		status = "disabled";
		compatible = "cirrus,cs35l43";
		#sound-dai-cells = <0>;
		reg = <0x41>;
		VA-supply = <&mldo8>;
	};
	spk4: cs35l43@43 {
		status = "disabled";
		compatible = "cirrus,cs35l43";
		#sound-dai-cells = <0>;
		reg = <0x43>;
		VA-supply = <&mldo8>;
	};
	cs35l45_1: cs35l45@30 {
		status = "disabled";
		compatible = "cirrus,cs35l45";
		#sound-dai-cells = <0>;
		reg = <0x30>;
		VA-supply = <&mldo7>;
	};
	cs35l45_2: cs35l45@31 {
		status = "disabled";
		compatible = "cirrus,cs35l45";
		#sound-dai-cells = <0>;
		reg = <0x31>;
		VA-supply = <&mldo7>;
	};

	/* haptic */
	haptic1: cs40l26@43 {
		status = "disabled";
		compatible = "cirrus,cs40l26a";
		reg = <0x43>;
		reset-gpios = <&dw_gpio3 24 1>;
		irq-gpio = <&dw_gpio1 14 0x0>;
		interrupt-parent = <&dw_gpio1>;
		interrupts = <14 IRQ_TYPE_LEVEL_LOW>;
		VA-supply = <&mldo8>;
		//cirrus,vbbr-enable;
		cirrus,vbbr-thld-uv = <273000>;
		cirrus,vbbr-max-att-db = <9>;
		cirrus,vbbr-atk-step = <1>;
		cirrus,vbbr-atk-rate = <2>;
		cirrus,vbbr-wait = <1>;
		cirrus,vbbr-rel-rate = <5>;

		//cirrus,vpbr-enable;
		cirrus,vpbr-thld-uv = <2639000>;
		cirrus,vpbr-max-att-db = <9>;
		cirrus,vpbr-atk-step = <1>;
		cirrus,vpbr-atk-rate = <2>;
		cirrus,vpbr-wait = <1>;
		cirrus,vpbr-rel-rate = <5>;

		//cirrus,bst-dcm-en;

		cirrus,bst-ipk-microamp = <4500000>;
	};
};

