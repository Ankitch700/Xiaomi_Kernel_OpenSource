&dw_i2c2 {
	cs47l92: codec@1a {
		status = "okay";
		compatible = "cirrus,cs47l92";
		reg = <0x1a>;
		#address-cells = <1>;
		#size-cells = <0>;

		//codec-reset-gpios = <&dw_gpio4 4 1>;
		interrupt-parent = <&dw_gpio4>;
		interrupts = <1 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-controller;
		#interrupt-cells = <1>;

		clock-frequency = <12288000>, <32764>;
		clocks = <&xr_clk XR_CLK_CODEC>,
			<&xr_clk XR_CLK_32K_O3>;
		clock-names = "mclk1", "mclk2";

		AVDD-supply = <1>;
		DBVDD1-supply = <1>;
		DCVDD-supply = <1>;

		pinctrl-names = "default";
		pinctrl-0 = <&pinsettings>;
		pinsettings: pin-settings {
			aif1-pins {
				groups = "aif1";
				function = "aif1";
				bias-bus-hold;
			};
		};

		micvdd {
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
		};
		MICBIAS1 {
			cirrus,ext-cap = <0>;
			regulator-active-discharge = <1>;
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <2800000>;
		};
		MICBIAS2 {
			cirrus,ext-cap = <0>;
			regulator-active-discharge = <1>;
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
		};

		cirrus,accdet {
			#address-cells = <1>;
			#size-cells = <0>;
			acc@1 {
				reg = <1>;

				cirrus,micd-bias-start-time = <0x1>;
				cirrus,micd-pol-gpios = <0>;
				cirrus,micd-rate = <0x7>;
				cirrus,micd-detect-debounce-ms = <300>;
				cirrus,micd-clamp-mode = <0x0>;
				//cirrus,jd-use-jd2 = <0>;
				cirrus,hs-mic = <1>;

				cirrus,init-delay-ms = <30>;
				cirrus,micd-dbtime = <0x1>;
				cirrus,micd-timeout-ms = <1000>;
				cirrus,micd-force-micbias;
				cirrus,hpdet-short-circuit-imp = <2>;
				cirrus,micd-ranges = <
						116 226
						173 582
						321 115
						752 114
						1257 217
				>;
				cirrus,micd-software-compare;
				cirrus,micd-open-circuit-declare = <1>;
				//cirrus,jd-wake-time = <5000>;

				cirrus,hpdet-ext-res = <0>;

				/*MICDET1|MICDET2/HPOUTFB2|MICBIAS2A|micd-poll-gpio pin= NONE|HPGND=MICDET2*/
				cirrus,micd-configs = <0 1 4 0 1>;
				/*HPOUT1L|HPOUT1R*/
				cirrus,hpd-pins = <0 4 1 4>;
			};
		};
	};
};

