	vbat_ovp_threshold_ffc = <4580>;
	vbat_ovp_threshold_normal = <4530>;
	status = "okay";
};

&xm_uefi_bq27z561 {
	status = "okay";
	i2c-bus-id = <20>;
	i2c-addr = <0x55>;
	i2c-retry-max-cnt = <3>;

	i2c-err-default-vbat = <(-1)>;
};

&xm_uefi_sc6601 {
	status = "okay";
	i2c-bus-id = <6>;
	i2c-addr = <0x61>;
	i2c-retry-max-cnt = <3>;

	/* hw init */
	spmic,vbat = <4528>;
	spmic,charge-curr = <500>;
	spmic,iindpm-disable = <0>;
	spmic,input-curr-limit = <500>;
	spmic,ico-enable = <0>;
	spmic,iindpm-ico = <100>;
	spmic,precharge-volt = <1>;	/* 0:2900mv, 1:3000, 2:3100, 3:3200 */
	spmic,precharge-curr = <250>;
	spmic,vac_ovp = <6500>;
	spmic,vbus_ovp = <6500>;

	support-wireless-chg = <1>;

	cp,i2c-bus-id = <20>;
	cp,i2c-addr = <0x6e>;
	cp,ovpgate-ovp-reg = <0x03>;
	cp,ovpgate-ovp-val = <0x83>; /* set vusb_ovp to 14V */

	i2c-err-default-vbat = <(-1)>;

	wls-sleep-gpioid = <50>;	/* wireless sleep gpio */
};

&xm_uefi_chargerapp {
	status = "okay";
	disable-charger = <0>; /* 0: enable uefi charger app 1: disable */
	boot-kernel-volt-th = <3400>;
