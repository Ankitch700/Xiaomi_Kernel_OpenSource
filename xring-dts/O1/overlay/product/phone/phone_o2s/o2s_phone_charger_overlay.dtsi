	tbat-critical-resume-th = <550>;
	batt-low-uefi-shutdown-th = <3000>;
	fg-pmic-vbat-ratio = <1>;	/* vbatfg/vbatpmic = 1*/
	high-soc-boot-volt-th = <3150>;

	precharge-curr-mA = <470>;

	sdp-aicl = <500>;
	sdp-ichg = <500>;
	cdp-aicl = <1500>;
	cdp-ichg = <1500>;
	dcp-aicl = <1500>;
	dcp-ichg = <1500>;
