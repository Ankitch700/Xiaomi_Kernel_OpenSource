config PD_ADAPTER
    depends on SOUTHCHIP_ADAPTER_CLASS
    bool "southchip PD adapter"
    default n
    help 
      Say Y to enable southchip pd adapter
config SOUTHCHIP_CHARGER_MANAGER
	tristate "support southchip charger manger"
	default n
	help
		Say Y here to support southchip charger manger.

config SOUTHCHIP_DVCHG_POLICY
	tristate "support southchip charger pump policy"
	default n
	help
		Say Y here to support southchip charger class.
